{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "dfd:png": "mmdc -i docs/diagrams/level0-context.mmd -o docs/diagrams/png/level0-context.png && mmdc -i docs/diagrams/level1-admin.mmd -o docs/diagrams/png/level1-admin.png && mmdc -i docs/diagrams/level1-student.mmd -o docs/diagrams/png/level1-student.png && mmdc -i docs/diagrams/level1-auth-data.mmd -o docs/diagrams/png/level1-auth-data.png",
    "dfd:svg": "mmdc -i docs/diagrams/level0-context.mmd -o docs/diagrams/svg/level0-context.svg && mmdc -i docs/diagrams/level1-admin.mmd -o docs/diagrams/svg/level1-admin.svg && mmdc -i docs/diagrams/level1-student.mmd -o docs/diagrams/svg/level1-student.svg && mmdc -i docs/diagrams/level1-auth-data.mmd -o docs/diagrams/svg/level1-auth-data.svg",
    "dfd:build": "node -e \"require('fs').mkdirSync('docs/diagrams/png',{recursive:true});require('fs').mkdirSync('docs/diagrams/svg',{recursive:true})\" && npm run dfd:png && npm run dfd:svg"
  },
